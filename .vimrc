filetype plugin indent on
set autoindent
set smartindent
set tabstop=4
set shiftwidth=4
set softtabstop=4
autocmd FileType HTML,javascript set shiftwidth=2 | set tabstop=2 | set softtabstop=2 set expandtab
set number
set laststatus=2
highlight StatusLine cterm=bold ctermfg=yellow ctermbg=blue
let &colorcolumn="80"
res +80
vertical res +20et &colorcolumn="80"    
res +80    
vertical res +20  
